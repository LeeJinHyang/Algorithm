    "def solution(numbers):\n",
    "    answer = []\n",
    "    \n",
    "    for i in range(0, len(numbers)-1):\n",
    "        for j in range(i+1, len(numbers)):\n",
    "            if (numbers[i]+numbers[j]) not in answer:\n",
    "                answer.append(numbers[i]+numbers[j])\n",
    "    answer.sort()\n",
    "    return answer"
   ]
